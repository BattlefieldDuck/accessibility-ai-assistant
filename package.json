{
	"name": "accessibility-ai-assistant",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:watch": "npx nodemon --watch src --watch static --exec \"vite build\" -e json,js,ts,css,html,svelte",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/chrome": "^0.0.283",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.65",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.460.1",
		"paneforge": "^1.0.0-next.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-adapter-chrome-extension": "^2.0.1",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.9",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@types/dom-chromium-ai": "^0.0.4"
	}
}
